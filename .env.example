# AWS Amplify Configuration
# Copy this file to .env.local and fill in your actual values

# AWS Region
NEXT_PUBLIC_AWS_REGION=us-east-1

# Cognito User Pool
NEXT_PUBLIC_USER_POOL_ID=us-east-1_xxxxxxxxx
NEXT_PUBLIC_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# Cognito Identity Pool (optional, for guest access)
NEXT_PUBLIC_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# GraphQL API (when DataStore is configured)
NEXT_PUBLIC_GRAPHQL_ENDPOINT=https://xxxxxxxxxxxxxxxxxxxxxxxxxx.appsync-api.us-east-1.amazonaws.com/graphql

# S3 Storage (for ambient sounds and exports)
NEXT_PUBLIC_S3_BUCKET=zenfocus-storage-bucket

# Optional: Environment identifier
NEXT_PUBLIC_ENVIRONMENT=development